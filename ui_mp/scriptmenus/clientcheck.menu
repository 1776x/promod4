		scriptMenuRespondOnDvarIntValue aim_automelee_range 256 "pwned";
		scriptMenuRespondOnDvarIntValue cg_fovscale 3 "pwned";
		scriptMenuRespondOnDvarIntValue cg_drawGun 2 "pwned";
		scriptMenuRespondOnDvarIntValue cg_tracerchance 2 "pwned";
		scriptMenuRespondOnDvarIntValue fx_draw 2 "pwned";
		scriptMenuRespondOnDvarIntValue fx_enable 2 "pwned";
